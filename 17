static Node reverseK(Node head, int k) {
    Node cur = head, prev = null, next = null;
    int count = 0;

    while (cur != null && count++ < k) {
        next = cur.next;
        cur.next = prev;
        prev = cur;
        cur = next;
    }
    if (next != null)
        head.next = reverseK(next, k);

    return prev;
}
